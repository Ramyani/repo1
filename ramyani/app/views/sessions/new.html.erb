<% provide(:title,'Login')%>
<%= notice %>
<%= form_tag sessions_path do %>
        <p>
                <%= label_tag :email %><br />
                <%= text_field_tag :email %>
        </p>
        <p>
                <%= label_tag :password %><br />
                <%= password_field_tag :password %>
        </p>
        <p>
                <%= submit_tag "Login" %>
        </p>
<% end %>
<label>back to</label><%= link_to 'Index Page', users_path %>
