<h2>List of All Users</h2>
<ol>

<% @users.each do |u| %>
<li><%= u.user_name %>
	<%= link_to 'Show', user_path(u) %>
    	<%= link_to 'Edit', edit_user_path(u) %>
   	<%= link_to 'Delete', user_path(u), :method => :delete, data: { confirm: 'Are you sure to delete this user?' } %>

</li>
<% end %>
</ol>
<%= link_to 'sign in', new_session_path %>for existing users
<br>
<%= link_to 'sign up', new_user_path %>for new user
